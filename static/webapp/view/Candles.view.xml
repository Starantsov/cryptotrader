<mvc:View controllerName="fokin.crypto.controller.Candles"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m">
	<Page>
		<content>
			<Table items="{data>Candles}"
				inset="false"
				growingScrollToLoad="false"
				growingThreshold="100"
				growing="true">
				<columns>
					<Column>
						<Text text="Time"/>
					</Column>
					<Column hAlign="End">
						<Text text="Close"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<ObjectIdentifier title="{data>moment}"/>
							<ObjectNumber number="{data>close}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>
