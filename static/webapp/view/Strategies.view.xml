<mvc:View controllerName="fokin.crypto.controller.Strategies"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m">
	<Page>
		<content>
			<List items="{data>/Strategies}" itemPress="onItemPress">
				<headerToolbar>
					<Toolbar>
						<content>
							<Title text="Strategies"/>
							<ToolbarSpacer/>
							<Button icon="sap-icon://add" press="onAdd"/>
						</content>
					</Toolbar>
				</headerToolbar>
				<items>
					<StandardListItem title="{data>name}" type="Active"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>
